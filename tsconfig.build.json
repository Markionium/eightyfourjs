{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "pretty": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "lib": ["es2017"],
    "paths": {},
    "declaration": false,
    "declarationMap": false,
    "outDir": "./lib",
    "rootDir": "./src",
    "resolveJsonModule": true
  },
  "include": ["src", "typings"],
  "references": []
}
